{
  "env": {
    "PATH": "$(PATH):$(HOME)/.local/bin"
  },
  "apps": [
    {
      "name": "Desktop",
      "image-path": "desktop.png",
      "prep-cmd": [
        {
          "do": "hyprctl --instance 0 keyword monitor eDP-1,1920x1080,auto,1",
          "undo": "hyprctl --instance 0 keyword monitor eDP-1,preferred,auto,1"
        }
      ]
    }
  ]
}
